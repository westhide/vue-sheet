{
  "name": "vue3-sheet",
  "version": "0.1.0",
  "description": "Office Excel like Vue Sheet",
  "author": "westhide",
  "homepage": "www.tibetourism.work:4173/login",
  "scripts": {
    "dev": "vite",
    "check": "vue-tsc --noEmit && pnpm check:wasm",
    "check:wasm": "cargo check --manifest-path ./wasm-rs/Cargo.toml",
    "build": "pnpm build:wasm && pnpm check && vite build",
    "build:wasm": "wasm-pack build ./wasm-rs --target web",
    "preview": "vite preview",
    "lint": "eslint \"{src,cypress}/**/*.{vue,ts,tsx,json}\" --fix && stylelint \"**/*.{vue,less,postcss,css,scss}\" --fix",
    "lint-staged": "lint-staged",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:cypress": "npx cypress open",
    "benchmark": "ts-node benchmark",
    "webstorm-integration": "vitest --watch --reporter=dot --reporter=json --outputFile=.vitest-result.json",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.{vue,scss,less,css,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@commitlint/types": "^17.0.0",
    "@iconify/json": "^2.1.78",
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@intlify/unplugin-vue-i18n": "^0.4.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vitejs/plugin-legacy": "^2.0.0",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vitest/ui": "^0.18.0",
    "@vue/test-utils": "^2.0.2",
    "@vueuse/core": "^8.9.3",
    "@vueuse/integrations": "^8.9.3",
    "autoprefixer": "^10.4.7",
    "axios": "^0.27.2",
    "benny": "^3.7.1",
    "c8": "^7.11.3",
    "change-case": "^4.1.2",
    "cross-env": "^7.0.3",
    "cypress": "^10.3.0",
    "dayjs": "^1.11.3",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.2.0",
    "husky": "^8.0.0",
    "jsdom": "^20.0.0",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "lodash-es": "^4.17.21",
    "nanoid": "^4.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.16",
    "postcss": "^8.4.14",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.7.1",
    "rollup": "^2.77.0",
    "rollup-plugin-visualizer": "^5.7.1",
    "rxjs": "^7.5.6",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "tailwindcss": "^3.1.6",
    "terser": "^5.14.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "universal-cookie": "^4.0.4",
    "unplugin-auto-import": "^0.9.3",
    "unplugin-icons": "^0.14.7",
    "unplugin-vue-components": "^0.21.1",
    "vite": "^3.0.0",
    "vite-plugin-banner": "^0.3.0",
    "vite-plugin-chunk-split": "^0.2.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-inspect": "^0.6.0",
    "vite-plugin-pages": "^0.25.0",
    "vite-plugin-vue-layouts": "^0.6.0",
    "vite-plugin-wasm-pack": "^0.1.12",
    "vitest": "^0.18.1",
    "vue-eslint-parser": "^9.0.3",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.1.2",
    "vue-tsc": "^0.38.0"
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china"
  }
}
